{
  "manifest_version": 2,
  "name": "Manipulate video",
  "version": "2021.5",
  "content_scripts": [
    {
	      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": ["numeric-1.2.6.min.js","content.js"]
    }
  ],
  "permissions": ["tabs","activeTab"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Manipulate video"
  },
  	"icons": {
        "128": "icon.png"
    }
}
